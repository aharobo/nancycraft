<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="tech.hidouding.webone.dao.SongMapper">

    <insert id="addNewSong" useGeneratedKeys="true" keyProperty="id">
        INSERT INTO song (title,author,duration)
        VALUES (#{title},#{author},#{duration})
    </insert>

</mapper>